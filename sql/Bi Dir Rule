    SELECT
        T.PERSON_ID,
        T.CONTACT_PERSON_ID,
        T.CONTACT_TYPE,
        LEAST(T.PERSON_ID, T.CONTACT_PERSON_ID) AS PAIR_LOW,
        GREATEST(T.PERSON_ID, T.CONTACT_PERSON_ID) AS PAIR_HIGH
    FROM DQ_PER_CONTACT_RELATIONSHIPS_V T
    JOIN DQ_PER_CONTACT_RELATIONSHIPS_V R ON R.PERSON_ID = T.CONTACT_PERSON_ID AND R.CONTACT_PERSON_ID = T.PERSON_ID
    WHERE 
        EXISTS (
  SELECT 1
  FROM DQ_CLEANSE_RESULTS R
  WHERE R.START_VALUE   =T.PERSON_ID
    AND R.PROCEDURE_NAME = 'DQ_EXISTS'
    AND R.FIELD_NAME     = 'DQ_PERSON_ID'
    AND R.BATCH_SEQ = (
          SELECT MAX(BATCH_SEQ)
          FROM DQ_CLEANSE_RESULTS
          WHERE DOMAIN_NAME = 'HR'
        )
);
